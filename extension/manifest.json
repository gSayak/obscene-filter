{
    "manifest_version": 3,
    "name": "Global-Censorship",
    "description": "Image and text censorship for NSFW/ 18+ contents for all websites",
    "version": "1.0",
    "icons":
    {
        "16": "./assets/icon.png",
        "48": "./assets/icon.png",
        "128": "./assets/icon.png"
    },
    "browser_action":
    {
        "default_icon": "./assets/icon.png",
        "default_popup": "./scripts/popup.html"
    },
    "permissions": [],
    "content_scripts":[
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./scripts/contentScript.js"]
        }
    ]
}